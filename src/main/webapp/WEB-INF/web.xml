<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <!--FileServlet-->
  <servlet>
    <servlet-name>ServletFile</servlet-name>
    <servlet-class>abdul.restApi.servlets.ServletFile</servlet-class>
  </servlet>
  <!--UserServlet-->
  <servlet>
    <servlet-name>ServletUser</servlet-name>
    <servlet-class>abdul.restApi.servlets.ServletUser</servlet-class>
  </servlet>
  <!--EventServlet-->
  <servlet>
    <servlet-name>ServletEvent</servlet-name>
    <servlet-class>abdul.restApi.servlets.ServletEvent</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>ServletFile</servlet-name>
    <url-pattern>/file</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>ServletEvent</servlet-name>
    <url-pattern>/event</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>ServletUser</servlet-name>
    <url-pattern>/user</url-pattern>
  </servlet-mapping>
</web-app>
